{
  "workflowApiVersion": "1.1",
  "metaData": {
    "icon": "https://forall.es/msd/icon.png",
    "category": "custom",
    "isConfigured": false,
    "configurationDisabled": false,
    "configOnDrop": false
  },
  "type": "RESTDECISION",
  "lang": {
    "es-ES": {
      "name": "RESTDECISION Activity",
      "description": "An example RESTDECISION Activity for Validate an Email"
    }
  },
  "arguments": {
    "executionMode": "{{Context.ExecutionMode}}",
    "definitionId": "{{Context.DefinitionId}}",
    "activityId": "{{Activity.Id}}",
    "contactKey": "{{Context.ContactKey}}",
    "execute": {
      "inArguments": [
        {
          "emailAddress": "{{InteractionDefaults.Email}}",
          "contactKey": "{{Context.ContactKey}}",
          "apiKey" : "",
          "contentBlock" : ""
        }
      ],
      "url": ""
    },
    "startActivityKey": "{{Context.StartActivityKey}}",
    "definitionInstanceId": "{{Context.DefinitionInstanceId}}",
    "requestObjectId": "{{Context.RequestObjectId}}"
  },
  "configurationArguments": {
    "save": "",
    "publish": {
      "url": "https://testcustomactivity.free.beeceptor.com"
    },
    "unpublish": "",
    "stop": "",
    "partnerActivityId": "",
    "validate": "",
    "outArgumentSchema": "",
    "executeSchema": ""
  },
  "userInterfaces": {
    "configModal": {
      "height": 600,
      "width": 1200,
      "fullscreen": false
    }
  },
  "editable": true,
  "outcomes": [
    {
      "key": null,
      "next": "RESTDECISION-2",
      "arguments": {
        "branchResult": "valid",
        "some argument": "Email validated by Everest"
      },
      "metaData": {
        "label": "valid",
        "invalid": false
      }
    },
    {
      "key": null,
      "next": "WAITBYDURATION-9",
      "arguments": {
        "branchResult": "invalid",
        "some argument": "The email is invalid according to Everest"
      },
      "metaData": {
        "label": "invalid",
        "invalid": false
      }
    }
  ],
  "errors": null,
  "schema": {
    "arguments": {
      "execute": {
        "inArguments": [
          {
            "contactKey": {
              "dataType": "Text",
              "isNullable": false,
              "direction": "in"
            }
          },
          {
            "emailAddress": {
              "dataType": "Email",
              "isNullable": false,
              "direction": "in"
            }
          },
          {
            "apiKey": {
              "dataType": "Text",
              "isNullable": false,
              "direction": "in"
            }
          },
          {
            "contentBlock" : {
              "dataType": "Text",
              "isNullable": true,
              "direction": "in"
            }
          }
        ],
        "outArguments": [
          {
            "foundSignupDate": {
              "dataType": "Date",
              "direction": "out",
              "access": "visible"
            }
          }
        ]
      }
    }
  }
}